<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pbinas.driveroo">

    <application
        android:name=".DriverooApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DriverTimeBooking">
        <activity android:name=".ui.browse.BrowseActivity"></activity>
        <activity android:name=".ui.details.DetailsActivity" />
        <activity android:name=".ui.chooseCountry.ChooseCountryActivity" />
        <activity android:name=".ui.chooseTime.ChooseTimeActivity" />
        <activity android:name=".ui.addNewEntry.AddNewEntryMenuActivity" />
        <activity android:name=".ui.mainMenu.view.MainMenuActivity" />
        <activity
            android:name=".ui.main.view.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.DriverTimeBooking.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>